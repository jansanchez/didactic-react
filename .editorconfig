root=true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.js,*.css]
indent_size = 4
indent_style = tab

[*.jsx]
indent_size = 4
indent_style = tab
insert_final_newline = true
